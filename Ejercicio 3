package examendeyimi;

import java.util.Scanner;
        
public class EjerciciodeYimi3 {
    public static void main(String[] args) {
        Scanner cs = new Scanner(System.in);
        int mayor;
        int menor;
        int medio=0;
        System.out.println("Ingrese el primer numero");
        int uno=cs.nextInt();
        System.out.println("Ingrese el segundo numero");
        int dos=cs.nextInt();
        System.out.println("Ingrese el terer numero");
        int tres=cs.nextInt();
            if (uno>dos &&  uno>tres) {
                mayor=uno;
                if (dos>tres){
                    medio=dos;
                    menor=tres;
                    }
                }else{
                    medio=dos;
                    menor=tres;
            }
            if (dos>uno && dos>tres){
                mayor=dos;
                if(uno>tres){
                    medio=uno;
                    menor=tres;
                    }
                }else{
                    medio=tres;
                    menor=uno;
            }
            if (tres>uno && tres>uno){
                mayor=tres;
                if(tres>uno){
                    medio=dos;
                    menor=uno;
                }
            }
                System.out.println("El numero central es: "+medio);
               
    }
}
